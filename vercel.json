{
  "version": 2,
    "builds": [
      {
        "src": "src/components/**/*",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/components/(.*)",
        "dest": "/src/components/$1"
      },
      {
        "src": "/api/:path*",
        "dest": "/app/api/:path*"
      },
      {
        "src": "/api/data/cards/(.*)",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "dest": "/api/data/cards/$1"
      },
      {
        "src": "/api/data/tabs/(.*)",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "dest": "/api/data/tabs/$1"
      }
    ]
}